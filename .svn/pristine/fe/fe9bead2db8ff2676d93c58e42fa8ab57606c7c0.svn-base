<UserControl
    x:Class="TicketMachine.WS.Helper.LargeBarcode"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TicketMachine.WS"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300">

    <StackPanel Margin="20,20,20,20">
        <TextBlock x:Name="barcodeBlock" Margin="0,0,0,18" FontFamily="../Fonts/Archon Code 39 Barcode.ttf#Archon Code 39 Barcode" FontSize="135" Text="" />
        <Button x:Name="Defaultbutton" Content="" Width="100" Margin="0,14,0,0" Click="close_click" Background="#ecf0f1" x:Uid="btnContentCloseLargeBarcode" HorizontalAlignment="Right" />
    </StackPanel>
</UserControl>
